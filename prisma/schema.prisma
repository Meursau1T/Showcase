generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model main_page {
  banner  String  @id @db.VarChar(255) @default("/test_banner.jpg")
  products Json   @default("[]")
}

model product {
  id       Int       @id @default(autoincrement())
  name String
  type String
  hlw  String
  manufacturer Json
  oem_no Json
  ref_no Json
  machine_model Json
  desc_app String
  price String
  cu_m3 String
  desc_zh String
  desc_en String
}

model category {
  id       Int       @id @default(autoincrement())
  /** 数组 */
  types    Json
  /** 数组 */
  types_en Json
}

model culture {
  id       Int       @id @default(autoincrement())
  /** 对象 */
  data    Json
}

model brand {
  id       Int       @id @default(autoincrement())
  /** 对象 */
  data    Json
}

model profile {
  id       Int       @id @default(autoincrement())
  /** 对象 */
  data    Json
}

model structure {
  id       Int       @id @default(autoincrement())
  /** 对象 */
  data    Json
}

model patents {
  id       Int       @id @default(autoincrement())
  /** 对象 */
  data    Json
}

model user {
  name String @id
  password String
}

