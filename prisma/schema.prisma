generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model main_page {
  banner  String  @id @db.VarChar(255) @default("/test_banner.jpg")
}

model product {
  name String @id
  type String
}

model category {
  id       Int       @id @default(autoincrement())
  types    Json
  types_en Json
}

model culture {
  id       Int       @id @default(autoincrement())
  data    Json
}

model brand {
  id       Int       @id @default(autoincrement())
  data    Json
}

model profile {
  id       Int       @id @default(autoincrement())
  data    Json
}

model structure {
  id       Int       @id @default(autoincrement())
  data    Json
}

model patents {
  id       Int       @id @default(autoincrement())
  data    Json
}

model user {
  name String @id
  password String
}

